<core:FragmentDefinition 
    xmlns:core="sap.ui.core"
	xmlns:table="sap.ui.table"
    xmlns:smartField="sap.ui.comp.smartfield" 
    xmlns="sap.m">
	
    <table:Column width="150px">
		<Label text="{i18n>reasonForRej}" />
        <table:customData>
            <core:CustomData key="p13nData"
				value='\{"columnKey": "ABGRU_VA", "columnIndex" : "101"}' />
        </table:customData>
        <table:template>
            <smartField:SmartField value="{ABGRU_VA}" editable="true"/>
        </table:template>
	</table:Column>

    <table:Column width="150px">
		<Label text="{i18n>cancelDate}" />
        <table:customData>
            <core:CustomData key="p13nData"
				value='\{"columnKey": "FSH_CANDATE", "columnIndex" : "101"}' />
        </table:customData>
        <table:template>
            <smartField:SmartField value="{FSH_CANDATE}" editable="true"/>
        </table:template>
	</table:Column>

</core:FragmentDefinition>
